{
  "name": "the-legend-of-zelda-a-link-to-the-past-randomizer-tracker",
  "description": "A Tracker for The Legend of Zelda: A Link to the Past Randomizer",
  "version": "0.1.0",
  "main": "src/app.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eliasthompson/the-legend-of-zelda-a-link-to-the-past-randomizer-tracker.git"
  },
  "babel": {
    "presets": [
      "react",
      "stage-2",
      [
        "env",
        {
          "useBuiltIns": "entry",
          "targets": {
            "browsers": [
              "last 2 versions"
            ],
            "uglify": true
          }
        }
      ]
    ],
    "plugins": [
      "transform-runtime",
      [
        "styled-jsx/babel",
        {
          "plugins": [
            "styled-jsx-plugin-sass"
          ]
        }
      ]
    ]
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "^4.17.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.1",
    "node-sass": "^4.5.3",
    "styled-jsx-plugin-sass": "^0.2.4"
  },
  "dependencies": {
    "prop-types": "^15.6.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.4",
    "styled-jsx": "^2.2.6"
  }
}
